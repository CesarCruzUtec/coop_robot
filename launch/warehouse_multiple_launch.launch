<?xml version="1.0"?>
<launch>
  <arg name="model" default="$(env TURTLEBOT3_MODEL)" />
  <arg name="x_pos1" default="-3.0" />
  <arg name="y_pos1" default="1.0" />
  <arg name="z_pos1" default="0.0" />
  <arg name="x_pos2" default="3.0" />
  <arg name="y_pos2" default="1.0" />
  <arg name="z_pos2" default="0.0" />
  <arg name="gui" default="false" />

  <include file="$(find aws_robomaker_small_warehouse_world)/launch/no_roof_small_warehouse.launch">
    <arg name="gui" value="$(arg gui)" />
  </include>

  <group ns="first_robot">
    <param name="robot_description"
      command="$(find xacro)/xacro --inorder $(find turtlebot3_description)/urdf/turtlebot3_$(arg model).urdf.xacro" />
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"
      args="-urdf -model turtlebot3 -x $(arg x_pos1) -y $(arg y_pos1) -z $(arg z_pos1) -param robot_description" />
  </group>

  <group ns="second_robot">
    <param name="robot_description"
      command="$(find xacro)/xacro --inorder $(find turtlebot3_description)/urdf/turtlebot3_$(arg model).urdf.xacro" />
    <node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"
      args="-urdf -model turtlebot3 -x $(arg x_pos2) -y $(arg y_pos2) -z $(arg z_pos2) -param robot_description" />
  </group>

  <!--include file="$(find turtlebot3_gazebo)/launch/turtlebot3_gazebo_rviz.launch">
    <arg name="model" value="$(arg model)" />
  </include-->
</launch>